<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="udesk.sdk.definedview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ScratchActivity"
            android:label="@string/title_activity_scratch" />
        <activity
            android:name=".DragActivity"
            android:label="@string/title_activity_drag" />
        <activity
            android:name=".DrawerActivity"
            android:label="@string/title_activity_drawer" />
        <activity
            android:name=".DragDownActivity"
            android:label="@string/title_activity_drag_down" />
        <activity android:name=".MarqueeActivity"
            android:label="@string/title_activity_drag_down" />
        <activity
            android:name=".LuckWheelActivity"
            android:label="@string/title_activity_luck_wheel" />
        <activity
            android:name=".LuckActivity"
            android:label="@string/title_activity_luck_wheel" />
    </application>

</manifest>